// 1
datasource db {
  provider = "sqlite" 
  url = "***"
}

// 2
generator client {
  provider = "prisma-client-js"
}

// 3
model Live {
    id            Int      @id @default(autoincrement())
    title         String
    description   String
    questionnaire Questionnaire?
}

model Questionnaire {
    id            Int      @id @default(autoincrement())
    title         String
    description   String
    liveId        Int
    live          Live    @relation(fields: [liveId], references: [id])
}
